<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <View
        android:id="@+id/rectangle_2"
        android:layout_width="256dp"
        android:layout_height="67dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="42dp"
        android:layout_marginTop="721dp"
        android:background="@drawable/rectangle_toast"
        android:elevation="6dp"
        />

    <View
        android:id="@+id/rectangle_image"
        android:layout_width="59dp"
        android:layout_height="57dp"
        android:layout_toEndOf="@id/rectangle_2"
        android:layout_marginStart="10dp"
        android:layout_marginTop="726dp"
        android:background="@drawable/rectangle_toast"
        />

    <View
        android:id="@+id/vector"
        android:layout_width="match_parent"
        android:layout_height="86dp"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@id/rectangle_image"
        android:layout_marginStart="10dp"
        android:layout_marginTop="363dp"
        android:layout_marginEnd="25dp"
        android:background="#FBFBFB"
        />

    <TextView
        android:id="@+id/withdrawn_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rectangle_image"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="@string/withdrawn_2"
        android:textAppearance="@style/withdrawn_2"
        />

    <TextView
        android:id="@+id/john_doe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/withdrawn_2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="@string/john_doe"
        android:textAppearance="@style/john_doe"
        />
</RelativeLayout>
